// Actual time limit number for each game mode
#define TIME_LIMIT_ITEM( gamemode ) \
	itemDef \
	{ \
		type			ITEM_TYPE_TEXT \
		visible when	(dvarstring( g_gametype ) == gamemode) \
		rect			0 0 128 24 HORIZONTAL_ALIGN_SUBLEFT VERTICAL_ALIGN_TOP \
		origin			ORIGIN_WARMUP_TIME_LIMIT \
		textfont		UI_FONT_NORMAL \
		textscale		WARMUP_TEXTSIZE_MEDIUM \
		textalign		ITEM_ALIGN_BOTTOM \
		dvar			scr_##gamemode##_timelimit \
		decoration \
	}

// All the time limit text for each game mode
TIME_LIMIT_ITEM( dm )
TIME_LIMIT_ITEM( tdm )
TIME_LIMIT_ITEM( as )
TIME_LIMIT_ITEM( koth )
TIME_LIMIT_ITEM( os )
TIME_LIMIT_ITEM( be )
TIME_LIMIT_ITEM( vs )

