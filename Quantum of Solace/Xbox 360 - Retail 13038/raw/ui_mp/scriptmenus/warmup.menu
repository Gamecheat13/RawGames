// Actual score limit number for each game mode that supports it
#define SCORE_LIMIT_ITEM( gamemode ) \
	itemDef \
	{ \
		type			ITEM_TYPE_TEXT \
		rect			0 0 128 24 HORIZONTAL_ALIGN_SUBLEFT VERTICAL_ALIGN_TOP \
		origin			ORIGIN_WARMUP_SCORE_LIMIT_TEXT \
		forecolor		0.6 0.6 0.6 1.0 \
		text			"@MPUI_SCORE_LIMIT_COLON" \
		textfont		UI_FONT_NORMAL \
		textscale		WARMUP_TEXTSIZE_SMALL \
		textalign		ITEM_ALIGN_LEFT \
		textalign		ITEM_ALIGN_BOTTOM \
		visible	when	(dvarstring(g_gametype) == gamemode && dvarint(scr_##gamemode##_scorelimit) != 0) \
		decoration \
	} \
	itemDef \
	{ \
		type			ITEM_TYPE_TEXT \
		visible when	(dvarstring( g_gametype ) == gamemode && dvarint(scr_##gamemode##_scorelimit) != 0) \
		rect			0 0 128 24 HORIZONTAL_ALIGN_SUBLEFT VERTICAL_ALIGN_TOP \
		origin			ORIGIN_WARMUP_SCORE_LIMIT \
		textfont		UI_FONT_NORMAL \
		textscale		WARMUP_TEXTSIZE_MEDIUM \
		textalign		ITEM_ALIGN_BOTTOM \
		dvar			scr_##gamemode##_scorelimit \
		decoration \
	}

SCORE_LIMIT_ITEM( dm )
SCORE_LIMIT_ITEM( tdm )
SCORE_LIMIT_ITEM( as )
SCORE_LIMIT_ITEM( koth )
SCORE_LIMIT_ITEM( os )