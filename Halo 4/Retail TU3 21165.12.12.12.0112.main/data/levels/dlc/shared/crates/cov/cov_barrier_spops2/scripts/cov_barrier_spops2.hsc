// FUNCTIONS ---------------------------------------------------------------------------------------
script startup instanced random_damage()

	begin_random_count( 1 )
		object_set_variant( this, 'left_major' );
		object_set_variant( this, 'mid_major' );
		object_set_variant( this, 'right_major' );
		object_set_variant( this, 'left_mid_major' );
		object_set_variant( this, 'mid_right_major' );
		object_set_variant( this, 'left_right_major' );
		object_set_variant( this, 'all_major' );
	end

end
