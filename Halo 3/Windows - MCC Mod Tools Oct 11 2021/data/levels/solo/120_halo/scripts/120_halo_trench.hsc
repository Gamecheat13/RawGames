(script static void (tunnel_fall_off_random (object obj_name1)(object obj_name2)(object obj_name3))
	(set random_num (random_range 0 3))
	(cond
		((= random_num 0)
			(object_damage_damage_section obj_name1 "fall" 1)
			(sleep 30)
			(object_damage_damage_section obj_name1 "destroy" 1)
		)
		((= random_num 1)
			(object_damage_damage_section obj_name2 "fall" 1)
			(sleep 30)
			(object_damage_damage_section obj_name2 "destroy" 1)
		)
		((= random_num 2)
			(object_damage_damage_section obj_name3 "fall" 1)
			(sleep 30)
			(object_damage_damage_section obj_name3 "destroy" 1)
		)
	)		
)

