(script dormant trench_run_events
;	(object_set_collision_damage_armor_scale 10)
	(cond
		((<= (game_coop_player_count) 2)
			(begin 
				(ai_place warthog01)
				(ai_place secret_egg 1)
				(object_cannot_die (ai_vehicle_get_from_starting_location warthog01/driver) true)
				(object_cannot_take_damage (ai_vehicle_get_from_starting_location warthog01/driver))
				(object_set_collision_damage_armor_scale (ai_vehicle_get_from_starting_location warthog01/driver) 10)
				(object_set_collision_damage_armor_scale (ai_vehicle_get_from_starting_location secret_egg/mongoose) 10)
				(unit_falling_damage_disable (ai_vehicle_get_from_starting_location secret_egg/mongoose) true)			
				(unit_falling_damage_disable (ai_vehicle_get_from_starting_location warthog01/driver) true)
				(object_cannot_die (ai_vehicle_get_from_starting_location secret_egg/mongoose) true)
				(object_cannot_take_damage (ai_vehicle_get_from_starting_location secret_egg/mongoose))			
				(if (not (game_is_cooperative))
					(begin
						(ai_vehicle_reserve_seat (ai_vehicle_get_from_starting_location warthog01/driver) "warthog_d" TRUE)
						(ai_vehicle_reserve_seat (ai_vehicle_get_from_starting_location secret_egg/mongoose) "mongoose_p" TRUE)
					)
				)
			)
		)
		((>= (game_coop_player_count) 3) 
			(begin
				(game_can_use_flashlights FALSE)
				(ai_place warthog01)
				(object_cannot_die (ai_vehicle_get_from_starting_location warthog01/driver) true)
				(object_cannot_take_damage (ai_vehicle_get_from_starting_location warthog01/driver))
				(unit_falling_damage_disable (ai_vehicle_get_from_starting_location warthog01/driver) true)
				(object_set_collision_damage_armor_scale (ai_vehicle_get_from_starting_location warthog01/driver) 10)		
				(ai_place warthog02)
				(object_cannot_die (ai_vehicle_get_from_starting_location warthog02/driver) true)
				(object_cannot_take_damage (ai_vehicle_get_from_starting_location warthog02/driver))
				(object_set_collision_damage_armor_scale (ai_vehicle_get_from_starting_location warthog02/driver) 10)
				(unit_falling_damage_disable (ai_vehicle_get_from_starting_location warthog02/driver) true)
				(ai_place secret_egg 2)
				(object_set_collision_damage_armor_scale (ai_vehicle_get_from_starting_location secret_egg/mongoose) 10)
				(object_set_collision_damage_armor_scale (ai_vehicle_get_from_starting_location secret_egg/mongoose02) 10)
		     	(unit_falling_damage_disable (ai_vehicle_get_from_starting_location secret_egg/mongoose) true)			
				(object_cannot_die (ai_vehicle_get_from_starting_location secret_egg/mongoose) true)
				(object_cannot_take_damage (ai_vehicle_get_from_starting_location secret_egg/mongoose))
		     	(unit_falling_damage_disable (ai_vehicle_get_from_starting_location secret_egg/mongoose02) true)			
				(object_cannot_die (ai_vehicle_get_from_starting_location secret_egg/mongoose02) true)
				(object_cannot_take_damage (ai_vehicle_get_from_starting_location secret_egg/mongoose02))												
			)
		)
	)

	(cond 
		((= (game_difficulty_get_real) easy) (set trench_speed 55))	
		((= (game_difficulty_get_real) normal) (set trench_speed 45))
;		((= (game_difficulty_get_real) heroic) (set trench_speed 15))
		((= (game_difficulty_get_real) legendary) (set trench_speed 25))		
	)
	(cond 
		((= (game_difficulty_get_real) easy) (set tunnel_speed 75))	
		((= (game_difficulty_get_real) normal) (set tunnel_speed 60))
		((= (game_difficulty_get_real) legendary) (set tunnel_speed 40))				
	)

	(wake trench_run_100)
	(wake trench_run_110)
	(wake trench_run_120)
	(wake trench_tunnel01)
	(wake trench_run_global)
	(wake plate_management)
	(wake trench_run_game_save)
	(sleep_until (volume_test_players 120_08_trench_encounter_100)5)
	(wake big_explosions_ambient)

	(wake trench01_navpoint_active)
	(if (>= (game_coop_player_count) 2)
		(begin
			(ai_place chopper)
			(ai_place chopper02)
		)
	)
	(ai_dialogue_enable FALSE)
	
)

(script continuous arbiter_in_vehicle
	(sleep_until
	(and
		(= (game_coop_player_count) 1)
		(or
			(vehicle_test_seat_list (ai_vehicle_get_from_starting_location warthog01/driver) "warthog_d" (player0))
			(vehicle_test_seat_list (ai_vehicle_get_from_starting_location secret_egg/mongoose) "mongoose_d" (player0))
		)			

		(= (unit_in_vehicle (unit obj_arbiter)) FALSE)
			
	)1)
	(if (and (vehicle_test_seat_list (ai_vehicle_get_from_starting_location warthog01/driver) "warthog_d" (player0))
		   (not (vehicle_test_seat_list (ai_vehicle_get_from_starting_location warthog01/driver) 
		   "warthog_g" obj_arbiter))
   	   
		)
		(begin
			(ai_vehicle_reserve_seat (ai_vehicle_get_from_starting_location warthog01/driver) "warthog_d" TRUE)
			(unit_set_active_camo (unit obj_arbiter) true 0)			
			(sleep 1)
			(vehicle_load_magic (ai_vehicle_get_from_starting_location warthog01/driver) "warthog_g" obj_arbiter)
			(sleep 1)
			(unit_set_active_camo (unit obj_arbiter) false 3)
		)

	)
	(if (and (vehicle_test_seat_list (ai_vehicle_get_from_starting_location secret_egg/mongoose) "mongoose_d" (player0))
			(not (vehicle_test_seat_list (ai_vehicle_get_from_starting_location secret_egg/mongoose) 
		   "mongoose_p" obj_arbiter))

		 )
		(begin
			(ai_vehicle_reserve_seat (ai_vehicle_get_from_starting_location secret_egg/mongoose) "mongoose_p" TRUE)
			(unit_set_active_camo (unit obj_arbiter) true 0)
			(sleep 1)
			(vehicle_load_magic (ai_vehicle_get_from_starting_location secret_egg/mongoose) "mongoose_p" obj_arbiter)
;			(ai_vehicle_enter_immediate arbiter_trench/trench secret_egg/mongoose "mongoose_p")		
			(sleep 1)
			(unit_set_active_camo (unit obj_arbiter) false 3)
		)
	)
	(sleep_until (or (= (unit_in_vehicle (player0)) FALSE)
		   	(not (vehicle_test_seat_list (ai_vehicle_get_from_starting_location secret_egg/mongoose) 
		   "mongoose_p" obj_arbiter))
		    (not (vehicle_test_seat_list (ai_vehicle_get_from_starting_location warthog01/driver) 
		   "warthog_g" obj_arbiter))
		  		) 1)
)

