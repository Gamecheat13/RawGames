    )
    (sleep (ai_play_line arbiter 050mc_160))
    (sleep 10)
    (if dialogue
        (print "arbiter: make short work of this abomination!")
    )
    (sleep (ai_play_line arbiter 050mc_170))
    (set g_playing_dialogue false)
    (ai_dialogue_enable true)
)

