(script static boolean players_in_a_vehicle
    (if (= (game_coop_player_count) 1)
        (unit_in_vehicle (player0))
        (if (= (game_coop_player_count) 2)
            (and (unit_in_vehicle (player0)) (unit_in_vehicle (player1)))
            (if (= (game_coop_player_count) 3)
                (and (unit_in_vehicle (player0)) (unit_in_vehicle (player1)) (unit_in_vehicle (player2)))
                (if (= (game_coop_player_count) 4)
                    (and (unit_in_vehicle (player0)) (unit_in_vehicle (player1)) (unit_in_vehicle (player2)) (unit_in_vehicle (player3)))
                )
            )
        )
    )
)

