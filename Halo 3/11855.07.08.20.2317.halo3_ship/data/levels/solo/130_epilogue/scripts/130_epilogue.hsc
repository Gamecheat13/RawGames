    (if debug
        (print "epilogue")
    )
    (switch_zone_set "epilogue_voi")
    (sleep 1)
    (cinematic_snap_to_black)
    (sleep 1)
    (if (cinematic_skip_start)
        (begin
            (if debug
                (print "130lb_earth")
            )
            (130lb_earth)
        )
    )
    (cinematic_skip_stop)
    (cinematic_show_letterbox_immediate false)
    (camera_control true)
    (if (cinematic_skip_start)
        (begin
            (sleep 30)
            (wake epilogue_camera)
            (sleep 1)
            (sound_impulse_start "sound\music\end_credits\credits_music" none 0)
            (fade_in 0 0 0 15)
            (sleep 30)
            (play_credits_unskippable)
            (sleep (+ 10538 15))
            (stop_bink_movie)
            (sleep_forever epilogue_camera)
            (fade_out 0 0 0 60)
            (sleep 60)
            (switch_zone_set "epilogue_space")
            (sleep 1)
            (if (cinematic_skip_start)
                (begin
                    (sleep 30)
                    (cinematic_snap_to_black)
                    (if debug
                        (print "130lc_epilogue")
                    )
                    (130lc_epilogue)
                    (if (>= (game_difficulty_get) legendary)
                        (begin
                            (if debug
                                (print "130ld_legendary")
                            )
                            (130ld_legendary)
                        )
                    )
                )
            )
        )
    )
    (cinematic_skip_stop)
    (render_atmosphere_fog true)
    (end_mission)
)

