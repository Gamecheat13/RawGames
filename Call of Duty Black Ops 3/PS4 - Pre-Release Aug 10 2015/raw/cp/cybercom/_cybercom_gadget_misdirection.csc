function init()
{
	init_clientfields();
}

function init_clientfields()
{
	// clientfield setup
	clientfield::register( "toplayer", "misdirection_enable", 1, 1, "int", &player_misdirectionToggle, !true, !true );
}

function player_misdirectionToggle( localClientNum, oldVal, newVal, bNewEnt, bInitialSnap, fieldName, bWasDemoJump )
{	
	misdirectionEnable( localClientNum, newVal );
}

