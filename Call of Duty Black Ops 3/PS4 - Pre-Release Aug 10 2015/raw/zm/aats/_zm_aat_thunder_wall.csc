#precache( "client_fx", "zombie/fx_bgb_respin_cycle_box_flash_zmb" );

#namespace zm_aat_thunder_wall;

function autoexec __init__sytem__() {     system::register("zm_aat_thunder_wall",&__init__,undefined,undefined);    }

function __init__()
{
	if ( !( isdefined( level.aat_in_use ) && level.aat_in_use ) )
	{
		return;
	}
	
	aat::register( "zm_aat_thunder_wall", "zmui_zm_aat_thunder_wall" );
	
	clientfield::register( "actor", "zm_aat_thunder_wall", 1, 1, "int", &zm_aat_thunder_wall_blast, !true, !true );
	
	level._effect[ "zm_aat_thunder_wall" ] = "zombie/fx_bgb_respin_cycle_box_flash_zmb";
}


function zm_aat_thunder_wall_blast( localClientNum, oldVal, newVal, bNewEnt, bInitialSnap, fieldName, bWasDemoJump )
{
	PlayFX( localClientNum, "zombie/fx_bgb_respin_cycle_box_flash_zmb", self.origin );
}

