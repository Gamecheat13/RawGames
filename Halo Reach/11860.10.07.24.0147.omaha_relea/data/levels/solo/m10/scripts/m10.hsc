(script static void start
    (print "start!")
    (if (= (game_insertion_point_get) 0)
        (ins_1stbowl)
        (if (= (game_insertion_point_get) 1)
            (ins_barn)
            (if (= (game_insertion_point_get) 2)
                (ins_meadow)
                (if (= (game_insertion_point_get) 3)
