(script dormant void twin_longsword_loop_start
    (sleep_until 
        (begin
            (device_set_position_track "dm_longswords" "m35_fp1" 0)
            (object_create "sc_longsword_01")
            (object_create "sc_longsword_02")
            (object_create "sc_longsword_03")
