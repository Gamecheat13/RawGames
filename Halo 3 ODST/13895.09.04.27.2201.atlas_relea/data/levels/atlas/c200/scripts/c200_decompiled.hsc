(script startup void c200_startup
    (fade_out 0 0 0 0)
    (ai_allegiance covenant player)
    (ai_allegiance player covenant)
    (ai_allegiance human player)
    (ai_allegiance player human)
    (ai_allegiance covenant human)
    (ai_allegiance human covenant)
    (chud_show_shield false)
    (if (and (not editor) (> (player_count) 0))
        (start)
        (fade_in 0 0 0 0)
    )
)

(script static void start
    (cinematic_snap_to_black)
    (if (= g_play_cinematics true)
        (begin
            (if (cinematic_skip_start)
                (begin
                    (if debug
                        (print "c200_cinematic")
                    )
                    (switch_zone_set "c200b")
                    (sleep 30)
                    (c200b)
                    (switch_zone_set "c300")
                    (sleep 30)
                    (cinematic_show_letterbox_immediate false)
                    (play_credits_unskippable)
                    (sleep (+ 10486 15))
                    (stop_bink_movie)
                    (sleep 60)
                    (cinematic_show_letterbox_immediate true)
                    (cinematic_set_title "title_1")
                    (sleep 60)
                    (cinematic_set_title "title_2")
                    (sleep 60)
                    (cinematic_set_title "title_3")
                    (sleep 180)
                    (c300)
                    (cinematic_snap_to_black)
                    (sleep 1)
                    (if (>= (game_difficulty_get) legendary)
                        (begin
                            (if debug
                                (print "legendary ending c400")
                            )
                            (switch_zone_set "l200_020")
                            (sleep 30)
                            (c400)
                        )
                    )
                )
            )
            (cinematic_skip_stop_internal)
        )
    )
    (sleep 1)
    (end_mission)
)

