(script dormant void md_100_dare_elevator
    (sleep_until (and (>= g_fa_obj_control 4) (volume_test_players "tv_md_100_dare_elevator") (volume_test_object "tv_md_100_dare_elevator" (ai_get_object "gr_buck_01")) (volume_test_object "tv_md_100_dare_elevator" (ai_get_object "gr_dare_01")) (= g_talking_active false) (= g_engineer_dead false)))
