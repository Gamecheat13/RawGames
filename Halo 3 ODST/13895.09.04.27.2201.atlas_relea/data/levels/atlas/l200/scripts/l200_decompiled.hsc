(script static void (f_arg_accessed (device_group terminal_group, string_id terminal_boolean, object_name arg_device_name, device arg_device))
    (device_group_set_immediate terminal_group 0)
    (pda_activate_marker player arg_device_name "arg_waypoints" false)
    (gp_boolean_set terminal_boolean true)
    (sleep 1)
    (gp_integer_set "gp_arg_index" (h100_arg_completed_short))
    (sleep 1)
    (object_set_vision_mode_render_default "arg_device_l200_01" true)
    (sleep 1)
    (if (device_arg_has_been_touched_by_unit arg_device (player0) 30)
        (set s_arg_player_id 0)
        (if (device_arg_has_been_touched_by_unit arg_device (player1) 30)
            (set s_arg_player_id 1)
            (if (device_arg_has_been_touched_by_unit arg_device (player2) 30)
                (set s_arg_player_id 2)
                (if (device_arg_has_been_touched_by_unit arg_device (player3) 30)
                    (set s_arg_player_id 3)
                )
            )
        )
    )
    (achievement_grant_to_all_players "_achievement_find_all_audio_logs")
    (sleep 1)
)

(script static void play_arg_element
    (if (= s_arg_player_id 0)
