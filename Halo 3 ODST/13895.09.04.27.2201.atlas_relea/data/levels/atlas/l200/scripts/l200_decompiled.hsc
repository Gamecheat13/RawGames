        (if (= s_arg_player_id 1)
            (pda_play_arg_sound (player1) "gp_arg_slot_30")
            (if (= s_arg_player_id 2)
                (pda_play_arg_sound (player2) "gp_arg_slot_30")
                (if (= s_arg_player_id 3)
                    (pda_play_arg_sound (player3) "gp_arg_slot_30")
                )
            )
        )
    )
)

