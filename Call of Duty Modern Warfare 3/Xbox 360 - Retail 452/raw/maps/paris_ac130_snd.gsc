add_context_sensitive_dialog( name1, name2, group, soundAlias )
{
	Assert( IsDefined( name1 ) );
	Assert( IsDefined( name2 ) );
	Assert( IsDefined( group ) );
	Assert( IsDefined( soundAlias ) );
	//Assert( SoundExists( soundAlias ) == true );

	if ( ( !IsDefined( level.scr_sound[ name1 ] ) ) || ( !IsDefined( level.scr_sound[ name1 ][ name2 ] ) ) || ( !IsDefined( level.scr_sound[ name1 ][ name2 ][ group ] ) ) )
	{
		// creating group for the first time
		level.scr_sound[ name1 ][ name2 ][ group ] = SpawnStruct();
		level.scr_sound[ name1 ][ name2 ][ group ].played = false;
		level.scr_sound[ name1 ][ name2 ][ group ].sounds = [];
	}

	//group exists, add the sound to the array
	index = level.scr_sound[ name1 ][ name2 ][ group ].sounds.size;
	level.scr_sound[ name1 ][ name2 ][ group ].sounds[ index ] = soundAlias;
}

add_context_sensitive_timeout( name1, name2, groupNum, timeoutDuration )
{
	if ( !IsDefined( level.context_sensitive_dialog_timeouts ) )
		level.context_sensitive_dialog_timeouts = [];

	createStruct = false;
	if ( !IsDefined( level.context_sensitive_dialog_timeouts[ name1 ] ) )
		createStruct = true;
	else if ( !IsDefined( level.context_sensitive_dialog_timeouts[ name1 ][ name2 ] ) )
		createStruct = true;
	if ( createStruct )
		level.context_sensitive_dialog_timeouts[ name1 ][ name2 ] = SpawnStruct();

	if ( IsDefined( groupNum ) )
	{
		level.context_sensitive_dialog_timeouts[ name1 ][ name2 ].groups = [];
		level.context_sensitive_dialog_timeouts[ name1 ][ name2 ].groups[ string( groupNum ) ] = SpawnStruct();
		level.context_sensitive_dialog_timeouts[ name1 ][ name2 ].groups[ string( groupNum ) ].v[ "timeoutDuration" ] = timeoutDuration * 1000;
		level.context_sensitive_dialog_timeouts[ name1 ][ name2 ].groups[ string( groupNum ) ].v[ "lastPlayed" ] = ( timeoutDuration * - 1000 );
	}
	else
	{
		level.context_sensitive_dialog_timeouts[ name1 ][ name2 ].v[ "timeoutDuration" ] = timeoutDuration * 1000;
		level.context_sensitive_dialog_timeouts[ name1 ][ name2 ].v[ "lastPlayed" ] = ( timeoutDuration * - 1000 );
	}
}