textures/normandy/walls/paper_fleur_wcoldpapr1_nodlight
{
	qer_editorimage textures/normandy/walls/paper@fleur_wcoldpapr1
	surfaceparm paper
	surfaceparm nodlight
	{
		map textures/normandy/walls/paper@fleur_wcoldpapr1
		rgbGen exactVertex
	}
	{
		map $lightmap
		blendFunc filter
	}
}

