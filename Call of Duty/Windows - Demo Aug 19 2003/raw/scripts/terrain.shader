
//
// This shader allows these textures, that you've specified per-vertex alpha on, to work in game.//
//



//
// Dirt Textures //
//



textures/normandy/ground/trench_base

{
	surfaceparm grass
	surfaceparm noncolliding
	polygonOffset
	qer_editorimage textures/normandy/ground/dirt@trench_goundbase.tga
	{
		map textures/normandy/ground/dirt@trench_goundbase
		rgbGen exactVertex
		alphagen vertex
		blendFunc blend
	nextbundle
		map $lightmap
		blendFunc filter
	}
}



