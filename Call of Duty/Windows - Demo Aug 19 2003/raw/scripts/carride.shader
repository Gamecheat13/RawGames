

textures/normandy/foliage/foliage_clamp_nrmdy_cntr
{
	surfaceparm foliage
	surfaceparm nomarks
	surfaceparm nolightmap

	qer_editorimage normandy/foliage/foliage_clamp@nrmdy_cntr.tga
	{
		map clampy textures/normandy/foliage/foliage_clamp@nrmdy_cntr.tga
		rgbgen vertex
		alphaFunc GE128
		depthWrite
	}
}




